{
    "name": "linnhtun/laravel-5-solarium",
    "description": "Laravel Framework package for using Solarium. Just changing illuminate/support dependency to 5.* to make it work on laravel 5.* and removed paging",
    "license" : "MIT",
    "authors": [
        {
            "name": "Zenek Palmer",
            "email": "zen.palmer@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "5.*",
        "solarium/solarium": "~3.2"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Fbf\\LaravelSolarium\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
